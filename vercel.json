{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "REACT_APP_SUPABASE_URL": "@react_app_supabase_url",
    "REACT_APP_SUPABASE_ANON_KEY": "@react_app_supabase_anon_key",
    "REACT_APP_CONTRACT_ADDRESS": "@react_app_contract_address",
    "REACT_APP_BACKEND_URL": "@react_app_backend_url",
    "REACT_APP_NETWORK_NAME": "@react_app_network_name",
    "REACT_APP_NETWORK_RPC_URL": "@react_app_network_rpc_url",
    "REACT_APP_NETWORK_CHAIN_ID": "@react_app_network_chain_id",
    "REACT_APP_NETWORK_CURRENCY_SYMBOL": "@react_app_network_currency_symbol"
  }
}
